{
  "name": "studynest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && stylelint src/**/*.*css && prettier -c .",
    "lint-fix": "eslint . --fix && stylelint src/**/*.*css --fix",
    "fmt": "prettier --write .",
    "preview": "vite preview --host",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
