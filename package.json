    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.10.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "~5.5.4",
